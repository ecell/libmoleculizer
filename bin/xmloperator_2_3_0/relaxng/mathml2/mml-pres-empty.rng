<grammar xmlns="http://relaxng.org/ns/structure/1.0">

<!-- Empty presentation schemata -->

<define name="mml.petoken">
 <choice>
  <ref name="mml.mspace.qname"/>
 </choice>
</define>

<define name="mml.mspace.qname">
 <element name="mspace">
  <empty/>
  <ref name="mml.Common.attrib"/>
  <ref name="mml.att-sizeinfo"/>
  <ref name="mml.att-linebreak"/>
 </element>
</define>

</grammar>
